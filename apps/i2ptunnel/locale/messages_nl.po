msgid "Previous"
msgstr "Vorige"

#: ../jsp/WEB-INF/classes/net/i2p/i2ptunnel/jsp/wizard_jsp.java:777
msgid "Save Tunnel"
msgstr "Tunnel opslaan"

#: ../jsp/WEB-INF/classes/net/i2p/i2ptunnel/jsp/wizard_jsp.java:783
